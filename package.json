{
  "name": "f11",
  "version": "0.1.2",
  "description": "F11 - The SnipeSocket",
  "main": "dist/src/index.min.js",
  "bin": {
    "f11": "bin/f11",
    "f11.js": "bin/f11.js"
  },
  "resourceFiles": [
    "msgs/derp-60.txt",
    "msgs/derp-100.txt",
    "msgs/glhf.txt",
    "msgs/remind.txt",
    "msgs/welcome.txt"
  ],
  "nexe": {
    "input": "dist/src/index.min.js",
    "output": "dist/src/test.nex",
    "resourceFiles": [
      "msgs/welcome.txt",
      "msgs/derp-60.txt"
    ],
    "browserify": {
      "excludes": [],
      "requires": []
    },
    "temp": "../src",
    "debug": true,
    "runtime": {
      "nodeConfigureOpts": [
        "--fully-static"
      ],
      "framework": "node",
      "version": "8.6.0"
    }
  },
  "scripts": {
    "ci": "npm run build && npm run compile && npm run compress",
    "start": "nodemon index.ts",
    "build": "tsc && terser dist/src/index.js > dist/src/index.min.js",
    "nexe": "nexe",
    "compile": "nexe -t 8.6.0 -i dist/src/index.min.js -o dist/f11 && du -sh dist/f11*",
    "compress": "([ ! -f dist/f11.xz ] || rm dist/f11.xz) && echo 'Compressing: dist/f11 -> dist/f11.xz' && xz -k -z -9 dist/f11 && du -sh dist/f11*"
  },
  "repository": {
    "type": "git",
    "url": "git@github-snipe:f11snipe/f11"
  },
  "keywords": [
    "node",
    "tcp",
    "snipe",
    "socket"
  ],
  "author": "F11snipe",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.25",
    "nexe": "^4.0.0-beta.19",
    "nodemon": "^2.0.15",
    "terser": "^5.12.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "colors": "^1.4.0"
  }
}
